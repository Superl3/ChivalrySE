function sumTeamRank() {
	local `{GAMEMODE}PlayerController PC;
	local int rnk;
	sumAgatha = 0;
	sumMason = 0;
	foreach WorldInfo.AllControllers(class'`{GAMEMODE}PlayerController', PC) {
		rnk = AOCPRI(PC.PlayerReplicationInfo).MyRank;
		if(AOCPRI(PC.PlayerReplicationInfo).GetCurrentTeam() == EFAC_MASON)
			sumMason = sumMason + rnk * rnk;
		if(AOCPRI(PC.PlayerReplicationInfo).GetCurrentTeam() == EFAC_AGATHA)
			sumAgatha = sumAgatha + rnk * rnk;
	}
}
function DeathBasedRankAB() {
	local `{GAMEMODE}PlayerController PC;
	local int sum, idx, i, rnk, rnkB, diff, keyAgatha, keyMason,lenA,lenB;

	sum = 0;
	keyAgatha = -1;
	keyMason = -1;
	Total.Length = 0;
	AgathaT.Length = 0;
	MasonT.Length = 0;
	diff = 100000;

	foreach WorldInfo.AllControllers(class'`{GAMEMODE}PlayerController', PC) {
		if(CanIBeRBAB(PC))
			continue;
		if(AOCPRI(PC.PlayerReplicationInfo).GetCurrentTeam() == EFAC_AGATHA)
			AgathaT.Additem(PC);
		else if(AOCPRI(PC.PlayerReplicationInfo).GetCurrentTeam() == EFAC_MASON)
			MasonT.Additem(PC);
	}

	for(idx = 0; idx < AgathaT.Length; idx++) {
		rnk = AOCPRI(AgathaT[idx].PlayerReplicationInfo).MyRank;
		sum += rnk * rnk;
	}
	for(idx = 0; idx < MasonT.Length; idx++) {
		rnk = AOCPRI(MasonT[idx].PlayerReplicationInfo).MyRank;
		sum -= rnk * rnk;
	}
	for(idx = 0; idx< AgathaT.Length;idx++) {
		if(AgathaT[idx].CurrentFamilyInfo.class == class'AOCFamilyInfo_Agatha_King')
			continue;
		rnk = AOCPRI(AgathaT[idx].PlayerReplicationInfo).MyRank;
		for(i = 0; i<MasonT.Length;i++) {		
			if(MasonT[i].CurrentFamilyInfo.class == class'AOCFamilyInfo_Mason_King')
				continue;
			rnkB = AOCPRI(MasonT[idx].PlayerReplicationInfo).MyRank;
			if(diff > abs(sum - rnk*rnk*2 + rnkB*rnkB*2)) {
				diff = abs(sum - rnk*rnk*2 + rnkB*rnkB*2);
				keyAgatha = idx;
				keyMason = i;
			}
		}
	}
	if(abs(AgathaT.Length - MasonT.Length) >= 1) {// trade or give
		if(AgathaT.Length > MasonT.Length) { 
			for(i = 0; i<AgathaT.Length;i++) {		
				if(AgathaT[i].CurrentFamilyInfo.class == class'AOCFamilyInfo_Agatha_King')
					continue;		
				rnk = AOCPRI(AgathaT[i].PlayerReplicationInfo).MyRank;
				if(diff > abs(sum - rnk*rnk*2)) {
					diff = abs(sum - rnk*rnk*2);
					keyAgatha = i;
					keyMason = -1;
				}
			}
		} else {
			for(i = 0; i<MasonT.Length;i++) {		
				if(MasonT[i].CurrentFamilyInfo.class == class'AOCFamilyInfo_Mason_King')
					continue;
				rnk = AOCPRI(MasonT[idx].PlayerReplicationInfo).MyRank;
				if(diff > abs(sum - rnk*rnk*2)) {
					diff = abs(sum - rnk*rnk*2);
					keyAgatha = -1;
					keyMason = i;
				}
			}
		}
	} 
	lenA = AgathaT.Length;
	lenB = MasonT.Length;
	if(keyAgatha != -1) {
		RankBalanceStack.RemoveItem(AgathaT[keyAgatha]);
		RankBalanceStack.AddItem(AgathaT[keyAgatha]);
		//RB_changeTeam(true,AgathaT[keyAgatha]);
		lenA -= 1;
		lenB += 1;
	}
	if(keyMason != -1) {
		RankBalanceStack.RemoveItem(MasonT[keyMason]);
		RankBalanceStack.AddItem(MasonT[keyMason]);
		//RB_changeTeam(false,MasonT[keyMason]);
		lenB -= 1;
		lenA += 1;
	}
	if(abs(lenA - lenB) >= 1) { // 아직까지 밸런스 문제 있을시
		DeathBasedRankAB();
	}
}
function RB_changeTeam( `{GAMEMODE}PlayerController PC) {
	local AOCFamilyInfo NewFamily;
	
	RankBalanceStack.RemoveItem(PC);

	if(AOCPRI(PC.PlayerReplicationInfo).GetCurrentTeam() == EFAC_AGATHA)
		NewFamily = AOCGRI(GameReplicationInfo).FamilyInfos[PC.CurrentFamilyInfo.default.ClassReference + 5];
	else 
		NewFamily = AOCGRI(GameReplicationInfo).FamilyInfos[PC.CurrentFamilyInfo.default.ClassReference];
	
	PC.ClientAutoBalance(NewFamily);
	LocalizedPrivateMessage(PC, 27);
	PC.SetNewClass(NewFamily, false, false);
	AOCPRI(PC.PlayerReplicationInfo).MyFamilyInfo = none;	

	AutoBalanceStack.RemoveItem(AOCPRI(PC.PlayerReplicationInfo));
	AutoBalanceStack.AddItem(AOCPRI(PC.PlayerReplicationInfo));
}
function RankBasedAutobalance() {
	local `{GAMEMODE}PlayerController PC;
	local int sumA, sumB, idx, i, rnk;
	foreach WorldInfo.AllControllers(class'`{GAMEMODE}PlayerController', PC) {
		PC.ClientDisplayConsoleMessage("RANKBASED START :::::::: ");
	}
	sumA = 0;
	sumB = 0;
	Total.Length = 0;
	AgathaT.Length = 0;
	MasonT.Length = 0;
	
	getPlayerList();
	for(idx = 0; idx < Total.Length; idx++) {
		rnk = AOCPRI(Total[idx].PlayerReplicationInfo).MyRank;
		if(AgathaT.Length - MasonT.Length > 2) { // 값을 *2로 계산
			sumB += rnk * rnk;
			MasonT.AddItem(Total[idx]);
		}
		else if(MasonT.Length - AgathaT.Length > 2) {
			sumA += rnk * rnk;
			AgathaT.AddItem(Total[idx]);
		}
		else {
			if(sumA > sumB) {
				sumB += rnk * rnk;
				MasonT.AddItem(Total[idx]);
			}
			else {
				sumA += rnk * rnk;
				AgathaT.AddItem(Total[idx]);
			}
		}
	}
	//inController.Response_JoinTeam(inTeam, true);
	for(idx = 0; idx < AgathaT.Length; idx++) {
		if(AOCPRI(AgathaT[idx].PlayerReplicationInfo).GetCurrentTeam() != EFAC_AGATHA)
		{
			//AOCPRI(AgathaT[idx].PlayerReplicationInfo).SetNextTeam(EFAC_AGATHA);
			//for(i = 0; i < ArrayCount(AOCGRI(GameReplicationInfo).FamilyInfos) && AOCGRI(GameReplicationInfo).FamilyInfos[i] != AgathaT[idx].CurrentFamilyInfo; ++i);
			//	i += -5;
			//ChangeTeam(AgathaT[idx], 1, true);
			if(AOCPRI(AgathaT[idx].PlayerReplicationInfo).GetCurrentClass() == ECLASS_Archer) {
				i = 0;
			}
			else if(AOCPRI(AgathaT[idx].PlayerReplicationInfo).GetCurrentClass() == ECLASS_ManAtArms) {
				i = 1;
			}
			else if(AOCPRI(AgathaT[idx].PlayerReplicationInfo).GetCurrentClass() == ECLASS_Vanguard) {
				i = 2;
			}
			else if(AOCPRI(AgathaT[idx].PlayerReplicationInfo).GetCurrentClass() == ECLASS_Knight) {
				i = 3;
			}
			AgathaT[idx].SetNewClass(AOCGRI(GameReplicationInfo).FamilyInfos[i]);
		}
	}
	for(idx = 0; idx < MasonT.Length; idx++) {
		if(AOCPRI(MasonT[idx].PlayerReplicationInfo).GetCurrentTeam() != EFAC_MASON)
		{
			//AOCPRI(MasonT[idx].PlayerReplicationInfo).SetNextTeam(EFAC_MASON);
			//for(i = 0; i < ArrayCount(AOCGRI(GameReplicationInfo).FamilyInfos) && AOCGRI(GameReplicationInfo).FamilyInfos[i] != AgathaT[idx].CurrentFamilyInfo; ++i);
			//	i += 5;
			//ChangeTeam(MasonT[idx], 2, true);
			if(AOCPRI(MasonT[idx].PlayerReplicationInfo).GetCurrentClass() == ECLASS_Archer) {
				i = 5;
			}
			else if(AOCPRI(MasonT[idx].PlayerReplicationInfo).GetCurrentClass() == ECLASS_ManAtArms) {
				i = 6;
			}
			else if(AOCPRI(MasonT[idx].PlayerReplicationInfo).GetCurrentClass() == ECLASS_Vanguard) {
				i = 7;
			}
			else if(AOCPRI(MasonT[idx].PlayerReplicationInfo).GetCurrentClass() == ECLASS_Knight) {
				i = 8;
			}
			MasonT[idx].SetNewClass(AOCGRI(GameReplicationInfo).FamilyInfos[i]);
		}
	}
	foreach WorldInfo.AllControllers(class'`{GAMEMODE}PlayerController', PC) {
		PC.ClientDisplayConsoleMessage("AGATHA LIST :::::::: ");
		for(idx=0;idx<AgathaT.Length;idx++) {
			rnk = AOCPRI(AgathaT[idx].PlayerReplicationInfo).MyRank;
			PC.ClientDisplayConsoleMessage(rnk);
		}
		
		PC.ClientDisplayConsoleMessage("MASON LIST :::::::: ");
		for(idx=0;idx<MasonT.Length;idx++) {
			rnk = AOCPRI(MasonT[idx].PlayerReplicationInfo).MyRank;
			PC.ClientDisplayConsoleMessage(rnk);
		}
	}
}

function int PCcomp(`{GAMEMODE}PlayerController A, `{GAMEMODE}PlayerController B)
{
	return AOCPRI(B.PlayerReplicationInfo).MyRank - AOCPRI(A.PlayerReplicationInfo).MyRank;
} 
